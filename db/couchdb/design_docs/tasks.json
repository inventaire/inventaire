{
  "_id": "_design/tasks",
  "language": "coffeescript",
  "views": {
    "bySuspectUri": {
      "map": "(doc)->\n  unless doc.state \n    emit doc.suspectUri, null"
    },
    "bySuggestionUri": {
      "map": "(doc)->\n  unless doc.state \n    emit doc.suggestionUri, null"
    },
    "byScore": {
      "map": "(doc)->\n  unless doc.state \n    emit [doc.externalSourcesOccurrences, doc.relationScore, doc.elasticScore], doc"
    }
  }
}