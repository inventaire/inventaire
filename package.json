    "americano": "^0.4.2",
    "blue-cot": "^3.3.0",
    "bluebird": "^3.4.0",
    "bluereq": "^2.1.1",
    "chalk": "^1.1.3",
    "coffee-script": "^1.12.2",
    "config": "^1.16.0",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^1.2.0",
    "cookie-signature": "^1.0.6",
    "couch-init2": "^2.1.5",
    "couch2elastic4sync": "^3.4.5",
    "credential": "^1.0.0",
    "email-addresses": "^2.0.1",
    "execa": "^0.4.0",
    "express-session": "^1.11.3",
    "follow": "^0.12.1",
    "formidable": "^1.0.17",
    "git-hooks": "^1.1.7",
    "gitlab-logging": "git+https://github.com/inventaire/node-gitlab-logging.git",
    "gm": "^1.22.0",
    "graceful-fs": "^4.1.2",
    "gravatar": "^1.4.0",
    "inv-couch": "^2.0.0",
    "inv-loggers": "^3.3.1",
    "inv-utils": "^2.0.4",
    "ip-regex": "^1.0.3",
    "isbn-groups": "^1.0.1",
    "isbn2": "^0.1.7",
    "jiff": "^0.7.3",
    "knox": "^0.9.2",
    "level-geospatial": "0.0.5",
    "level-party": "^3.0.4",
    "level-sublevel": "^6.5.2",
    "lodash": "^3.10.1",
    "mime": "^1.3.4",
    "moment": "^2.13.0",
    "node-polyglot": "^1.0.0",
    "nodemailer": "git+https://github.com/maxlath/Nodemailer.git",
    "nodemailer-express-handlebars": "^1.0.0",
    "passport": "^0.2.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "ps-tree": "~1.1.0",
    "request": "^2.72.0",
    "simple-uuid": "0.0.1",
    "wikidata-lang": "^2.0.3",
    "wikidata-sdk": "^4.1.0",
    "xml2js": "^0.4.13"
  },
  "devDependencies": {
    "coffee-errors": "^0.8.6",
    "coffeelint": "^1.15.7",
    "diff": "^2.2.1",
    "doctoc": "^1.2.0",
    "git-hooks": "^1.0.2",
    "level-test": "^2.0.1",
